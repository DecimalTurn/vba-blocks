#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
  *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/../vendor/node" ]; then
  "$basedir/../vendor/node" --no-warnings "$basedir/../lib/vbapm.js" "$@"
else
  node --no-warnings "$basedir/../lib/vbapm.js" "$@"
fi
exit $?
