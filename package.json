{
	"name": "vbapm",
	"version": "0.6.1-alpha",
	"description": "A package manager and build tool for VBA",
	"contributors": [
		"Tim Hall <tim.hall.engr@gmail.com>",
		"Martin Leduc <31558169+DecimalTurn@users.noreply.github.com>"
	],
	"license": "MIT",
	"homepage": "https://github.com/vbapm/core",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vbapm/core.git"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"bin": {
		"vba-blocks": "lib/vba-blocks.js",
		"vba": "lib/vba-blocks.js"
	},
	"main": "lib/index.js",
	"types": "lib/src/index.d.ts",
	"exports": {
		".": {
			"require": "./lib/index.js",
			"types": "./lib/src/index.d.ts"
		}
	},
	"files": [
		"lib/",
		"addins/build/",
		"run-scripts/",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"format": "prettier --write \"**/*.{ts,js}\"",
		"test": "jest --runInBand",
		"typecheck": "tsc",
		"test:e2e": "jest --config e2e.config.js --runInBand",
		"build": "rimraf lib && rollup -c && tsc -p tsconfig.build.json",
		"build:dev": "cross-env NODE_ENV=development rollup -c",
		"build:addins": "cd addins && node --no-warnings ../scripts/build-addins",
		"build:bootstrap": "cd scripts/bootstrap && node --no-warnings ../../lib/vba-blocks build",
		"prepublishOnly": "npm run build",
		"postversion": "git push && git push --tags",
		"clean": "rimraf lib && rimraf dist && rimraf addins/build"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node",
		"testPathIgnorePatterns": ["/node_modules/", "/lib/"]
	},
	"dependencies": {
		"@decimalturn/toml-patch": "^0.7.0",
		"@timhall/ansi-colors": "^5.0.0",
		"@timhall/dedent": "^0.9.1",
		"archiver": "^3",
		"conf": "^3",
		"date-fns": "^2",
		"debug": "^4",
		"decompress": "^4",
		"editorconfig": "^0.15.3",
		"env-paths": "^2",
		"fs-extra": "^7",
		"isomorphic-git": "^1.8.2",
		"logic-solver": "^2",
		"meant": "^1.0",
		"mri": "^1",
		"node-fetch": "^2",
		"open": "^6",
		"pretty-hrtime": "^1",
		"sanitize-filename": "^1",
		"semver": "^5",
		"tmp": "^0.0",
		"walk-sync": "^1",
		"xml-js": "^1.6"
	},
	"devDependencies": {
		"@octokit/core": "^3.1.4",
		"@octokit/rest": "^16.21.1",
		"@rollup/plugin-commonjs": "^15.1.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^9.0.0",
		"@rollup/plugin-replace": "^2.3.3",
		"@rollup/plugin-typescript": "^6.0.0",
		"@types/archiver": "^2",
		"@types/fs-extra": "^5",
		"@types/glob": "^7.2.0",
		"@types/jest": "^23",
		"@types/node": "^10",
		"@types/node-fetch": "^2.1.7",
		"@types/semver": "^5",
		"@types/tmp": "^0.0",
		"aws-sdk": "^2.562.0",
		"builtin-modules": "^3.0.0",
		"cross-env": "^5",
		"dotenv": "^7.0.0",
		"editor": "^1.0.0",
		"jest": "^26.6.1",
		"mustache": "^3",
		"npm-run-all": "^4",
		"prettier": "^1",
		"rimraf": "^2",
		"rollup": "^2.32.1",
		"rollup-plugin-terser": "^7.0.2",
		"ts-jest": "^26.4.2",
		"tslib": "^1.9.3",
		"typescript": "^4.9.5",
		"url-join": "^4.0.1"
	}
}
